{
  "name": "digital_audit_report_preparation",
  "version": "1.0.0",
  "description": "Node.js Rest Apis with Express, Sequelize & MySQL",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "test": "jest --runInBand",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:seed": "npx sequelize-cli db:migrate:undo:all && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "migration:create": "npx sequelize-cli migration:generate --name ",
    "seed:create": "npx sequelize seed:generate --name "
  },
  "keywords": [
    "nodejs",
    "express",
    "rest",
    "api",
    "sequelize",
    "mysql"
  ],
  "type": "module",
  "engines": {
    "node": ">=16",
    "npm": ">=6"
  },
  "prettier": {
    "jsxSingleQuote": true,
    "semi": false,
    "singleQuote": true
  },
  "transform": {
    "^.+\\.js$": "babel-jest"
  },
  "jest": {
    "testTimeout": 50000
  },
  "author": "bezkoder",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "i18n": "^0.15.3",
    "jsonwebtoken": "^9.0.2",
    "lodash.merge": "^4.6.2",
    "mariadb": "^3.3.1",
    "mysql2": "^3.11.0",
    "nodemailer": "^6.9.7",
    "nodemon": "^3.0.1",
    "puppeteer": "^24.34.0",
    "redis": "^5.10.0",
    "sequelize": "^6.37.3",
    "socket.io": "^4.7.2",
    "sqlite3": "^5.1.7",
    "supertest": "^6.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "sequelize-cli": "^6.6.2"
  }
}
